{
  "name": "vue3-template",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host",
    "lint": "npm run lint:js && pnpm run lint:style",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:style -- --fix",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "serve": "vite preview",
    "copyfiles": "copyfiles -u 2 node_modules/leaflet/dist/images/* public/images"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "^4.2.5",
    "vue3-google-map": "^0.18.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "autoprefixer": "^10.4.16",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tailwindcss": "^3.9.0",
    "eslint-plugin-vue": "^9.19.2",
    "leaflet": "^1.9.4",
    "pnpm": "^8.12.0",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "stylelint": "^15.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^30.0.1",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.10",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^5.3.0",
    "vite-svg-loader": "^5.1.0"
  }
}
